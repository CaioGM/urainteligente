<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
     <servlet>
        <servlet-name>TwilioServlet</servlet-name>
        <servlet-class>com.porto.ninjas.ws.TwilioServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TwilioServlet</servlet-name>
        <url-pattern>/twilioNinjas</url-pattern>
    </servlet-mapping>
    
      
     <servlet>
        <servlet-name>ninjas</servlet-name>
        <servlet-class>com.porto.ninjas.ws.UraServlet</servlet-class>
    </servlet>

    <servlet-mapping>	
        <servlet-name>ninjas</servlet-name>
        <url-pattern>/ninjas</url-pattern>
    </servlet-mapping>
    
</web-app>
